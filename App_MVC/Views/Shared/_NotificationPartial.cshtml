@if(TempData["success"]!=null)
{
    <script>
        toastr.success('@TempData["success"]')
    </script>
}
@if (TempData["info"] is not null)
{
    <script>
        toastr.info('@TempData["info"]')
    </script>
}
@if (TempData["warning"] is not null)
{
    <script>
        toastr.info('@TempData["warning"]')
    </script>
}
@if (TempData["Error"] is not null)
{
    <script>
        toastr.info('@TempData["Error"]')
    </script>
}




